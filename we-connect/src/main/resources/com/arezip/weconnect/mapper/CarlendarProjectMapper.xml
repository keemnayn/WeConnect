<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.arezip.weconnect.mapper.CarlendarProjectMapper">
	<select id="findProjectByMemberId"
		resultType="com.arezip.weconnect.model.dto.ProjectDTO">
		SELECT project_name, to_char(project_start, 'YYYY-MM-DD') as projectStart, to_char(project_end, 'YYYY-MM-DD') as projectEnd
		FROM project
		WHERE
		member_id = #{memberId}
	</select>
	<insert id="insertProject" parameterType="com.arezip.weconnect.model.dto.ProjectDTO">
		insert into project(project_id,project_name,project_start,project_end,member_ib) 
		values(project_seq.nextval,#{projectName},#{projectStart},#{projectEnd},#{memberId})
	</insert>
</mapper>