<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.arezip.weconnect.mapper.FreeBoardMapper">
<insert id="insertFreeBoard" parameterType="com.arezip.weconnect.model.vo.FreeBoardVO">
	insert into free_board(free_board_id,free_board_title,free_board_content,member_id)
	values(free_board_seq.nextval,#{title},#{content},#{memberId})
</insert>
<select id="getFreeBoardList" resultType="com.arezip.weconnect.model.vo.FreeBoardVO">
	select free_board_id,free_board_title,member_name,to_char(free_board_create,'YYYY-MM-DD')   
	from free_board join member on
	free_board.member_id = member.member_id
</select>
</mapper>