    <cl:group std:sid="group-da480a4f" class="cl-form-group">
      <cl:xylayoutdata std:sid="xyl-data-31c51576" top="270px" left="20px" width="140px" height="30px" horizontalAnchor="LEFT" verticalAnchor="TOP"/>
